<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="3dp"
    app:cardElevation="10dp"

    xmlns:app="http://schemas.android.com/apk/res-auto">
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/live_ll"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView android:textStyle="bold"
                android:textSize="18dp" android:id="@+id/tv"
                android:layout_width="match_parent"
                android:text="S.No."
                android:layout_alignParentStart="true"
                android:padding="10dp"
                android:gravity="center_vertical"
                android:background="#6607A1"

                android:textColor="@color/white"

                android:layout_height="wrap_content">  </TextView>
            <androidx.appcompat.widget.AppCompatButton
                android:layout_gravity="end" android:id="@+id/print_script_btn"
                android:layout_width="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_height="40dp"
                android:textAlignment="center"
                android:drawablePadding="5dp"
                android:layout_alignParentRight="true"
                android:background="#4A0078"
                android:padding="11dp" android:textStyle="bold"
                android:drawableRight="@drawable/passbook_selector"
                android:text="Print Receipt"
                android:textColor="@color/white" android:textColorLink="@color/white"
                android:textSize="12dp" />

        </RelativeLayout>



        <LinearLayout android:visibility="gone"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView android:textStyle="bold"
                    android:textSize="12dp"
                    android:layout_width="100dp"
                    android:text="S.No."
                    android:gravity="center_vertical"

                    android:textColor="@color/ared"

                    android:layout_height="match_parent">
                </TextView>
                <TextView android:textStyle="bold"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="MemberId"
                    android:gravity="center_vertical"

                    android:textSize="12dp"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView android:textStyle="bold"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Name"
                    android:gravity="center_vertical"
                    android:textSize="12dp"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:textColor="@color/ared"
                    android:text=""
                    android:gravity="center_vertical"
                    android:textSize="12dp"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:id="@+id/tv1"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:id="@+id/tv2"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout android:padding="5dp"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView android:textStyle="bold"
                    android:layout_width="100dp"
                    android:text="Access mode type"
                    android:gravity="center_vertical"
                    android:textSize="12dp" 



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView android:textStyle="bold"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Latitude &amp; Longitude"
                    android:gravity="center_vertical"

                    android:textSize="12dp"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView android:textStyle="bold"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Mobile No"
                    android:gravity="center_vertical"
                    android:textSize="12dp"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv3"
                    android:layout_width="100dp"

                    android:text=""
                    android:gravity="center_vertical"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv4"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:id="@+id/tv5"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>

        </LinearLayout>


        <LinearLayout android:padding="5dp"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:text="Iin No" android:textStyle="bold"
                    android:gravity="center_vertical"
                    android:textSize="12dp"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Aadhar No" android:textStyle="bold"
                    android:gravity="center_vertical"

                    android:textSize="12dp"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Amount" android:textStyle="bold"
                    android:gravity="center_vertical"

                    android:textSize="12dp"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>

            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv7"
                    android:layout_width="100dp"

                    android:text=""
                    android:gravity="center_vertical"


                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv6"
                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv8"
                    android:layout_height="match_parent">
                </TextView>

            </LinearLayout>

        </LinearLayout>


        <LinearLayout android:padding="5dp"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:text="Txn Type "
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:textStyle="bold"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Txn Status &amp; Txn No"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="12dp"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Txn Message"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="12dp"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>

            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv9"
                    android:layout_width="100dp"

                    android:text=""
                    android:gravity="center_vertical"


                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv10"
                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv11"
                    android:layout_height="match_parent">
                </TextView>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout android:padding="5dp"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:text="Bankrrn"
                    android:gravity="center_vertical"
                    android:textSize="12dp"

                    android:textStyle="bold"

                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Ack. No"
                    android:gravity="center_vertical"

                    android:textSize="12dp"
                    android:textStyle="bold"

                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="On date"
                    android:gravity="center_vertical"

                    android:textSize="12dp"

                    android:textStyle="bold"
                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>

            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv12"
                    android:layout_width="100dp"

                    android:text=""
                    android:gravity="center_vertical"


                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv13"
                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv14"
                    android:layout_height="match_parent">
                </TextView>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:background="@color/grey"
        android:layout_height="5dp">

    </View>
</LinearLayout>



</androidx.cardview.widget.CardView>
